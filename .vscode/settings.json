{
  "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.github": true,
      "**/.gitp*": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/.vscode": true,
      "**/core.Microsoft*": true,
      "**/core.mongo*": true,
      "**/core.python*": true,
      "**/CVS": true
   },
   "files.autoSave": "off",
   "workbench.colorTheme": "Visual Studio Dark",
   "terminal.integrated.gpuAcceleration": "canvas",
   "[python]": {
      "editor.defaultFoldingRangeProvider": "ms-python.pylint",
      "editor.defaultFormatter": "ms-python.pylint",
   },
   "pylint.args": [
      "\"pylint.args\": [\"load-plugins=pylint_django\"],"
   ]
   
}
